# readings of pressures and tempertures for the ID line  
# argon gas pressure for ID line
grecord(calcout,"$(P)ARGON_PRESSURE")  {
	field(INPA,"$(P)DVX_5.VAL")
	field(CALC,"(ABS(A)-0.5)*600.0")
	field(SCAN,"1 second")
	field(DESC,"argon pressure (psi)")
}

