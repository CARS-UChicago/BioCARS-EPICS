

grecord(busy,"$(P)beamCheckTbl") {
		field(DESC, "Beam Check T Start")
}

#IK added a record for the cancel button

grecord(busy,"$(P)beamCheckTblCancel") {
		field(DESC, "BeamCheck Cancel")
}


grecord(ao, "$(P)beamCheckTblDelta") {
 		field("VAL",".003")
 	    field(PREC,"4")
    	field(DESC, "Vert Tbl Increment")
}

grecord(ao, "$(P)beamCheckTblPkFd") {
 		field("VAL","0")
 	    field(PREC,"0")
    	field(DESC, "Peak Found")
}


grecord(ao, "$(P)beamCheckTblPeak") {
 	
 	    field(PREC,"5")
    	field(DESC, "Vert Tbl Peak Pos")
}

grecord(ao, "$(P)beamCheckTblIter") {
		field("VAL","5")
 	    field(PREC,"0")
    	field(DESC, "BeamCheckTbl  Iterations")
}




grecord(stringout, "$(P)beamCheckTblStatus") {
	field("VAL", "Idle")
}

# Added by RH
grecord(mbbo,"$(P)beamCheckDetector") {
	field(DESC,"Select detector")
	field(VAL,"0")
	field(ZRVL,"0")
	field(ZRST,"WaveSurfer I0")
	field(ONVL,"1")
	field(ONST,"Keithley I0")
}

