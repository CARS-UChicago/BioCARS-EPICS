# readings of analog encoders for Downstream Table
# T2DnY
grecord(calcout,"$(P)T2DnY")  {
	field(INPA,"$(P)IP330_1.VAL")
	##field(INPA,"$(P)userAve5.VAL")
	field(CALC,"((A)*1.0 +0.0)")
	field(PREC,"4")
	field(SCAN,"1 second")
	field(DESC,"T2 Downstream Y mm")
}
