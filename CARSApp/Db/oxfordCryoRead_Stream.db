record(ai,"$(P)oxTemp") {
	field(DESC, "Cryo temp")
	field(DTYP,"stream")
	field(INP,"@Cryo.proto getTemp $(PORT)")
	field(PREC, "1")
	field(SCAN, "4")
	field(FLNK, "$(P)oxSampleFlow.PROC")
}
record(ai,"$(P)oxSampleFlow") {
        field(DESC, "Sample flow")
	field(DTYP,"stream")
	field(INP,"@Cryo.proto getSampleFlow $(PORT)")
	field(PREC, "1")
	field(FLNK, "$(P)oxShieldFlow.PROC")
}
